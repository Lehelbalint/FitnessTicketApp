@model List<FitnessTicketApp.Models.Domain.CheckIns>
@{
}
<h1>My Check Ins</h1>
<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Client Id</th>
			<th>Ticket Id</th>
			<th>Check In Date</th>
			<th>Barcode</th>
			<th>Gym name</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var ticket in Model)
		{
			<tr>
				<td>@ticket.Id</td>
				<td>@ticket.ClientId</td>
				<td>@ticket.TicketId.ToString()</td>
				<td>@ticket.CheckInDate.ToString()</td>
				<td>@ticket.Barcode</td>
				<td>@ticket.GymId.ToString()</td>
			</tr>
		}
	</tbody>
</table>